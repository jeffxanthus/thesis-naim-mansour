function [ output_args ] = IRL1-EpsilonTest(clip,size,method)
%IRL1-EPSILONTEST 
global fFf

epsilons=[0.8 0.9 1 1.1 1.2 1.3];

SNRs=[];
SNRms=[];
for i=epsilons
    fFf=i;
    ra=(rand()+1)*100000;
    for j=[ra-10000 ra ra+10000]
        [dataOrig, reconstruction, dummy1, dummy2, SNR, SNRm]=Simulation(clip,size,method,ra);
        SNRs(i,:)=[SNRs(i,:) SNR];
        SNRms(i,:)=[SNRms(i,:) SNRm];
    end
end

subplot(2,1,1);plot(SNRs(1,:),'r',SNRs(1,:),'r'

end

